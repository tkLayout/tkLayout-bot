  elif [ "$aLayout" == "OT616_IT615" ]; then
      cat <<EOF     
                  <td> IT study. <br> Was based from IT614. <br> TFPX: Changed sensors spacing within each double-disk system. </td>                                                                                                               
EOF
  elif [ "$aLayout" == "OT616_IT616" ]; then
      cat <<EOF     
